<!DOCTYPE html>
<html lang="en">
<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon icon-->
	<link rel="shortcut icon" type="image/x-icon" href="{$basePath}/assets/images/favicon/favicon.ico">
	<!-- Libs CSS -->
	<link href="{$basePath}/assets/libs/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet">
	<link href="{$basePath}/assets/libs/dropzone/dist/dropzone.css"  rel="stylesheet">
	<link href="{$basePath}/assets/libs/@mdi/font/css/materialdesignicons.min.css" rel="stylesheet" />
	<link href="{$basePath}/assets/libs/prismjs/themes/prism-okaidia.css" rel="stylesheet">
	<!-- Theme CSS -->
	<link rel="stylesheet" href="{$basePath}/assets/css/theme.min.css">
	<title>Homepage | Dash Ui - Bootstrap 5 Admin Dashboard Template</title>
</head>

<body class="bg-light">
<div id="db-wrapper">
	<!-- navbar vertical -->
	<!-- Sidebar -->
	<nav class="navbar-vertical navbar">
		<div class="nav-scroller">
			{include '#sidebar.latte'}
		</div>
	</nav>
	<!-- Page content -->
	<div id="page-content">
		<div class="header @@classList">
			{include '#top.latte'}
		</div>

		<div n:if="$flashes" n:foreach="$flashes as $flash" class="alert alert-{$flash->type}">
			{$flash->message}
		</div>

		{include content}

		</div>
	</div>
</div>

<!-- Scripts -->
<!-- Libs JS -->
<script src="{$basePath}/assets/libs/jquery/dist/jquery.min.js"></script>
<script src="{$basePath}/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="{$basePath}/assets/libs/jquery-slimscroll/jquery.slimscroll.min.js"></script>
<script src="{$basePath}/assets/libs/feather-icons/dist/feather.min.js"></script>
<script src="{$basePath}/assets/libs/prismjs/prism.js"></script>
<script src="{$basePath}/assets/libs/apexcharts/dist/apexcharts.min.js"></script>
<script src="{$basePath}/assets/libs/dropzone/dist/min/dropzone.min.js"></script>
<script src="{$basePath}/assets/libs/prismjs/plugins/toolbar/prism-toolbar.min.js"></script>
<script src="{$basePath}/assets/libs/prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.min.js"></script>

<!-- Theme JS -->
<script src="{$basePath}/assets/js/theme.min.js"></script>

</body>

</html>